@model dynamic

<section class="span9">
    <ul class="thumbnails">
        <li ng-repeat="item in items" class="span2">
            <a href="" ng-click="select(item)" class="thumbnail" ng-show="hasChildren(item)">
                <img ng-src="{{'https://se.airshoppen.com' + item.imageHref}}" alt="">
                {{item.name}}
            </a>
            <div class="thumbnail" ng-show="!hasChildren(item)">
                <img ng-src="{{'https://se.airshoppen.com' + item.imageHref}}" alt="">
                {{item.name}}
            </div>
        </li>
    </ul>
</section>
<section class="span3">
    <table class="table">
        <thead>
            
        </thead>
        <tbody>
            <tr ng-repeat="item in basketItems">
                <td>{{item.name}}</td>
                <td>{{item.amount}}</td>
            </tr>
        </tbody>
    </table>
</section>